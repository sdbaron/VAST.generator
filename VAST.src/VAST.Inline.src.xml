                            <Tracking event="pause"><![CDATA[{{API-SERVER}}/event/pause]]></Tracking>
                            <Tracking event="resume"><![CDATA[{{API-SERVER}}/event/resume]]></Tracking>
                            <Tracking event="fullscreen"><![CDATA[{{API-SERVER}}/event/fullscreen]]></Tracking>
                            <Tracking event="expand"><![CDATA[{{API-SERVER}}/event/expand]]></Tracking>
                            <Tracking event="collapse"><![CDATA[{{API-SERVER}}/event/collapse]]></Tracking>
                            <Tracking event="close"><![CDATA[{{API-SERVER}}/event/close]]></Tracking>
                        </TrackingEvents>

                        <VideoClicks>
                            <ClickThrough><![CDATA[{{API-SERVER}}/event/videoClick]]></ClickThrough>
                        </VideoClicks>
                        <MediaFiles>
                            <MediaFile id="1" delivery="progressive" type="video/mp4" bitrate="512" maintainAspectRatio="true" scalable="true" width="640" height="360">
                                <![CDATA[ {{API-SERVER}}/video/lays.mp4 ]]>
                            </MediaFile>
