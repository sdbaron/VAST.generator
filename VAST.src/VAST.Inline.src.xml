<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<VAST version="2.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="http://specs.adfox.ru/uploads/vast.xsd">
    <Ad id="1.1">
        <InLine>
            <AdSystem>Videonow</AdSystem>
            <AdTitle/>
            <ViewableImpression>
                <Viewable>{{API-SERVER}}/event/ViewableImpression.viewable.1</Viewable>
                <Viewable>{{API-SERVER}}/event/ViewableImpression.viewable.2</Viewable>
            </ViewableImpression>
            <Error><![CDATA[{{API-SERVER}}/error/[ERRORCODE]?message=[MESSAGE]]]></Error>
            <Impression><![CDATA[{{API-SERVER}}/event/impression]]></Impression>
            <SafeInfo><![CDATA[{{API-SERVER}}/event/safeInfo?location=[LOCATION]&visibility=[VISIBILITY]&size=[SIZE]&sq=[SQUARE]]]></SafeInfo>
            <Creatives>
                <Creative>
                    <Linear>
                        <Duration>00:00:17</Duration>
                        <TrackingEvents>
                            <Tracking event="creativeView"><![CDATA[{{API-SERVER}}/event/creativeView]]></Tracking>
                            <Tracking event="start"><![CDATA[{{API-SERVER}}/event/start]]></Tracking>
                            <Tracking event="midpoint"><![CDATA[{{API-SERVER}}/event/midpoint]]></Tracking>
