                            <!--</MediaFile>-->
                            <!--<MediaFile id="2" delivery="progressive" type="video/mpeg" bitrate="512" width="404" height="720" maintainAspectRatio="true" scalable="true">-->
                            <!--<![CDATA[ {{API-SERVER}}/video/coffee.mpg]]>-->
                            <!--</MediaFile>-->
                            <!--<MediaFile id="3" delivery="progressive" bitrate="96" type="audio/mp3">-->
                            <!--<![CDATA[{{API-SERVER}}/video/VP_Coffee_404x720.mp3]]>-->
                            <!--</MediaFile>-->
                        </MediaFiles>
                    </Linear>
                </Creative>
            </Creatives>
            <Extensions>
                <Extension type="vnCreative">
                    <select>{{API-SERVER}}/event/creativeSelect/?id=1.1</select>
                    <debug>{{API-SERVER}}/event/creativeDebug/?id=1.1</debug>
                    <impression>{{API-SERVER}}/event/creativeImpression/?id=1.1</impression>
                    <passed>{{API-SERVER}}/event/creativePassed/?id=1.1</passed>
                </Extension>
            </Extensions>
